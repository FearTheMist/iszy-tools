{
  "name": "iszy_tools",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --cache --fix src/**/*.{js,vue}"
  },
  "dependencies": {
    "@ckpack/vue-color": "^1.1.4",
    "@highlightjs/vue-plugin": "^2.0.1",
    "@icon-park/vue-next": "^1.3.6",
    "@lucky-canvas/vue": "^0.1.3",
    "@supermap/iclient-common": "^10.2.0",
    "algoliasearch": "^4.10.5",
    "ali-oss": "^6.16.0",
    "ant-design-vue": "^3.0.0-beta.4",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "codemirror": "^5.62.0",
    "core-js": "^3.18.2",
    "dayjs": "^1.10.7",
    "dom-to-image": "^2.6.0",
    "esri-leaflet": "^3.0.3",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.2.0",
    "js-beautify": "^1.14.0",
    "jsoneditor": "^9.5.0",
    "leaflet": "^1.7.1",
    "localforage": "^1.9.0",
    "lodash-es": "^4.17.21",
    "marked": "^3.0.4",
    "relationship.js": "^1.0.0",
    "three": "^0.133.1",
    "tinycolor2": "^1.4.2",
    "ua-parser-js": "^0.7.28",
    "uuid": "^8.3.2",
    "vue": "^3.2.26",
    "vue-router": "^4.0.9",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/runtime-dom": "^3.2.20",
    "cesium": "^1.85.0",
    "co": "^4.6.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.17.0",
    "ghooks": "^2.0.4",
    "less": "^4.1.1",
    "rollup": "^2.52.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "sass": "^1.35.1",
    "sitemap": "^7.0.0",
    "validate-commit-msg": "^2.14.0",
    "vite": "^2.5.6",
    "vite-plugin-cesium": "^1.2.9",
    "vite-plugin-pwa": "^0.8.1",
    "vite-plugin-style-import": "^1.0.0",
    "workbox-build": "^6.1.5",
    "workbox-window": "^6.1.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/vue3-essential",
      "standard"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 0,
      "no-empty": 0,
      "no-template-curly-in-string": 0,
      "no-eval": 0,
      "no-useless-escape": 0,
      "no-loss-of-precision": 0,
      "no-unused-vars": 0
    }
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "",
      "autoFix": false
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "license": "MIT"
}
